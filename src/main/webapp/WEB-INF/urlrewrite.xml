<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 3.0//EN" "http://tuckey.org/res/dtds/urlrewrite3.0.dtd">
<urlrewrite default-match-type="wildcard">
	<rule> 
    	<from>/</from> 
   		<to>/app/welcome</to> 
   	</rule>
   	<rule> 
    	<from>/top.jsp</from> 
   		<to>/top.jsp</to> 
   	</rule>
   	<rule> 
    	<from>/center.jsp</from> 
   		<to>/center.jsp</to> 
   	</rule>
   	<rule> 
    	<from>/down.jsp</from> 
   		<to>/down.jsp</to> 
   	</rule>
   	<rule> 
    	<from>/middel.jsp</from> 
   		<to>/middel.jsp</to> 
   	</rule>
   	<rule> 
    	<from>/left.jsp</from> 
   		<to>/left.jsp</to> 
   	</rule>
   	<rule> 
    	<from>/right.jsp</from> 
   		<to>/right.jsp</to> 
   	</rule>
   	<rule> 
        <from>/CheckCode.svl</from> 
        <to>/CheckCode.svl</to> 
    </rule> 
   	<rule> 
        <from>/j_spring_security_check</from> 
        <to>/j_spring_security_check</to> 
    </rule> 
    <rule> 
        <from>/j_spring_security_logout</from> 
        <to>/j_spring_security_logout</to> 
    </rule>
	<rule>
		<from>/common/**</from>
		<to>/common/$1</to>
	</rule>
	<rule>
		<from>/js/**</from>
		<to>/js/$1</to>
	</rule>
	<rule>
		<from>/css/**</from>
		<to>/css/$1</to>
	</rule>
	<rule>
		<from>/images/**</from>
		<to>/images/$1</to>
	</rule>
	<rule>
		<from>/scripts/**</from>
		<to>/scripts/$1</to>
	</rule>
	<rule>
		<from>/styles/**</from>
		<to>/styles/$1</to>
	</rule>
	<rule>
		<from>/**</from>
		<to>/app/$1</to>
	</rule>
	<outbound-rule>
		<from>/app/**</from>
		<to>/$1</to>
	</outbound-rule>
</urlrewrite>
